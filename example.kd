import "std/io.kd";

fn main(): i32 {
    let out_file = File::open_write("file.txt");
    out_file.write("Hello, world!", 13);
    out_file.close();
    let in_file = File::open_read("file.txt");
    let map = in_file.create_map();
    write(1, map.ptr, map.len);
    map.destroy();
    in_file.close();
    return 0;
}
