import "std/syscall.lang";

type Foo = struct {
    a: i16;
    b: i32;
};

fn set(let foo: *mut Foo) {
    *foo = Foo{4, 5};
}

fn add(let foo: Foo): i32 {
    return foo.a + foo.b;
}

fn main(): i32 {
    write(1, "Hello, world!", 13);
    var foo = Foo{1, 2};
    set(&foo);
    return add(foo);
}
